#include "main.h"

/**
* _strcat - concatenates two strings
* @dest: copy to destination character
* @src: to source character
*/

char *_strcat(char *dest, char *src)
{
int i;
int j;

int i = 0;
while (dest[i] != '\0')
{
i++;
}
j = 0;
while (src[j] != '\0')
{
dest[i] = src[j];
i++;
j++;
}
dest[i] = '\0';
return (dest);
}
